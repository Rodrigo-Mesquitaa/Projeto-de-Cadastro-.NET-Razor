@page
@model InscricaoApp.Web.Pages.Pacotes.CreateModel

<h1>Criar Pacote</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Pacote.Descricao"></label>
        <input asp-for="Pacote.Descricao" class="form-control" />
        <span asp-validation-for="Pacote.Descricao" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Pacote.Preco"></label>
        <input asp-for="Pacote.Preco" class="form-control" />
        <span asp-validation-for="Pacote.Preco" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Salvar</button>
</form>
